{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "learningtech909/multi-client",
      "essential": false
    },
    {
      "name": "server",
      "image": "learningtech909/multi-server",
      "essential": true,
      "environment": [
        { "name": "PGUSER", "value": "postgres" },
        { "name": "PGPASSWORD", "value": "postgrespassword" },
        { "name": "PGHOST", "value": "multi-docker-postgres.cf4qwkaici9y.ap-south-1.rds.amazonaws.com" },
        { "name": "PGPORT", "value": "5432" },
        { "name": "PGDATABASE", "value": "fibvalues" },
        { "name": "REDIS_HOST", "value": "multi-docker-redis-4xhpjh.serverless.aps1.cache.amazonaws.com" },
        { "name": "REDIS_PORT", "value": "6379" }
      ]
    },
    {
      "name": "worker",
      "image": "learningtech909/multi-worker",
      "essential": false,
      "environment": [
        { "name": "REDIS_HOST", "value": "multi-docker-redis-4xhpjh.serverless.aps1.cache.amazonaws.com" },
        { "name": "REDIS_PORT", "value": "6379" }
      ]
    },
    {
      "name": "nginx",
      "image": "learningtech909/multi-nginx",
      "essential": false,
      "portMappings": [
        { "hostPort": 80, "containerPort": 80 }
      ]
    }
  ]
}
